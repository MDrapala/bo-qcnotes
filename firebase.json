{
  "hosting": {
    "public": "out",
    "target": "default",
    "appAssociation": "AUTO",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/500",
        "destination": "/500.html"
      },
      {
        "source": "/404",
        "destination": "/404.html"
      },
      {
        "source": "/auth",
        "destination": "/auth.html"
      },
      {
        "source": "/dashboard",
        "destination": "/dashboard.html"
      },
      {
        "source": "/classes",
        "destination": "/classes.html"
      },
      {
        "source": "/classes/:classId",
        "destination": "/classes/*.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
